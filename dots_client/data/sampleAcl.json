{
    "ietf-dots-data-channel:acls":{  
        "acl":[  
           {  
              "name":"sample-ipv4-acl",
              "type":"ipv4-acl-type",
              "activation-type":"immediate",
              "aces":{  
                 "ace":[  
                    {  
                       "name":"rule1",
                       "matches":{  
                          "ipv4":{
                             "source-ipv4-network":"192.168.17.0/24",
                             "destination-ipv4-network":"1.1.2.0/24",
                             "ttl":10,
                             "protocol":6,
                             "flags":"more",
                             "dscp": 3
                          },
                          "tcp":{
                              "destination-port":{
                                  "lower-port":443,
                                  "upper-port":445
                              }
                          }
                       },
                       "actions":{  
                          "forwarding":"accept"
                       }
                    }
                 ]
              }
           }
        ]
     }
 }